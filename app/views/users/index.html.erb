<div class="bs-example">
  <div class="btn-group btn-group-justified">
    <a class="btn btn-primary" role="button" id="tab_btn_followers" data-target="#tab_content_followers">Followers</a>
    <a class="btn btn-default" role="button" id="tab_btn_not-followers" data-target="#tab_content_not-followers">Not Followers</a>
